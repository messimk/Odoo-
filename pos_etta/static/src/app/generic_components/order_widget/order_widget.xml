<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="pos_etta.OrderWidget" t-inherit="point_of_sale.OrderWidget" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('order-summary')]" position="inside">
            <t t-if="orderlines.length > 0">
                <div class="fs-6 text-muted subentry">
                    Item #: <span t-esc="(product_total).toFixed(2)" class="items"/>
                </div>
            </t>
        </xpath>
    </t>
</templates>